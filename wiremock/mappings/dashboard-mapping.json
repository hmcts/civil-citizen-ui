{
  "mappings": [
    {
      "request": {
        "method": "POST",
        "urlPattern": "/scenarios/.*?/.*?",
        "headers": {
          "Content-Type": {
            "equalTo": "application/x-www-form-urlencoded"
          },
          "Authorization": {
            "matches": "Bearer .*"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ]
}
