    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.2.0",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.14",
    "@types/nunjucks": "^3.2.1",
    "@types/require-directory": "^2.1.1",
    "@types/serve-favicon": "^2.5.1",
    "@wdio/sauce-service": "^7.16.13",
    "applicationinsights": "^2.2.0",
    "axios": "^0.25.0",
    "config": "^3.3.2",
    "cookie-parser": "^1.4.6",
