{% from "./external-link.njk" import externalLink %}
{% import "external-urls.njk" as externalURLs %}

{% macro supportLinks(t, title, links) %}

  <p class="govuk-heading-m govuk-!-margin-bottom-2">{{ title }}</p>
  {% for link in links %}
    <p class="govuk-body govuk-!-margin-bottom-1">{{ externalLink(t, link.text, link.url, false, link.translated) }}</p>
  {% endfor %}

{% endmacro %}
